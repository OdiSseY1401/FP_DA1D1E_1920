Proceso Ejercicio14
	Definir num1, num2, suma, resta, producto, division Como Real;
	Escribir "Dime un número";
	Leer num1;
	Escribir "Dime un número";
	Leer num2;
	
	suma <- num1 + num2;
	resta <- num1 - num2;
	producto <- num1 * num2;
	division <- num1 / num2;
	
	Escribir "El resultado de la suma es :", suma;
	Escribir "El resultado de la resta es :", resta;
	Escribir "El resultado de la producto es :", producto;
	Escribir "El resultado de la division es :", division;
FinProceso
